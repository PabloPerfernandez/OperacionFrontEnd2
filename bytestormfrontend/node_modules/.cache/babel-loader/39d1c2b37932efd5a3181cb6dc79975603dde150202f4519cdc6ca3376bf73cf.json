os asignados -->\r\n                <v-card-text v-else>Sin equipos asignados</v-card-text>\r\n              </v-list-item-content>\r\n            </v-list-item>\r\n          </v-list>\r\n\r\n          <!-- Si no tiene misiones asignadas -->\r\n          <v-card-text v-else>Sin misiones asignadas</v-card-text>\r\n        </v-card>\r\n      </v-col>\r\n    </v-row>\r\n\r\n    <!-- Tabla de Operativos -->\r\n    <h2>Lista de Operativos</h2>\r\n    <v-row v-if=\"operativos.length > 0\" dense>\r\n      <v-col v-for=\"operativo in operativos\" :key=\"operativo.id\" cols=\"12\" sm=\"6\" md=\"4\">\r\n        <!-- Tarjeta de Operativo -->\r\n        <v-card outlined>\r\n          <v-card-title>\r\n            Operativo ID: {{ operativo.id }}\r\n          </v-card-title>\r\n          <v-card-subtitle>\r\n            Nombre: {{ operativo.nombre }}\r\n          </v-card-subtitle>\r\n          <v-card-text>\r\n            Rol: {{ operativo.rol }}\r\n          </v-card-text>\r\n\r\n          <!-- Botones de edición y eliminación para cada operativo -->\r\n          <v-card-actions>\r\n            <v-btn icon @click=\"editOperativo(operativo)\">\r\n              <v-icon>mdi-pencil</v-icon>\r\n            </v-btn>\r\n            <v-btn icon @click=\"deleteOperativo(operativo.id)\">\r\n              <v-icon>mdi-delete</v-icon>\r\n            </v-btn>\r\n          </v-card-actions>\r\n\r\n          <!-- Mostrar misiones del operativo -->\r\n          <v-list v-if=\"operativo.misiones && operativo.misiones.length > 0\">\r\n            <v-list-item v-for=\"mision in operativo.misiones\" :key=\"mision.codigo\">\r\n              <v-list-item-content>\r\n                <v-list-item-title>\r\n                  Código de Misión: {{ mision.codigo }}\r\n                </v-list-item-title>\r\n                <v-list-item-subtitle>\r\n                  Descripción: {{ mision.descripcion }}\r\n                </v-list-item-subtitle>\r\n\r\n                <!-- Mostrar equipos asociados a la misión -->\r\n                <v-list v-if=\"mision.equipos && mision.equipos.length > 0\">\r\n                  <v-list-item v-for=\"equipo in mision.equipos\" :key=\"equipo.equipoCodigo\">\r\n                    <v-list-item-content>\r\n                      <v-list-item-title>Equipo Código: {{ equipo.equipoCodigo }}</v-list-item-title>\r\n                      <v-list-item-subtitle>Tipo: {{ equipo.tipo }}</v-list-item-subtitle>\r\n                      <v-list-item-text>Descripción: {{ equipo.descripcion }}</v-list-item-text>\r\n                    </v-list-item-content>\r\n                  </v-list-item>\r\n                </v-list>\r\n\r\n                <!-- Si no tiene equipos asignados -->\r\n                <v-card-text v-else>Sin equipos asignados</v-card-text>\r\n              </v-list-item-content>\r\n            </v-list-item>\r\n          </v-list>\r\n\r\n          <!-- Si no tiene misiones asignadas -->\r\n          <v-card-text v-else>Sin misiones asignadas</v-card-text>\r\n        </v-card>\r\n      </v-col>\r\n    </v-row>\r\n\r\n    <v-divider class=\"my-5\"></v-divider>\r\n\r\n    <!-- FORMULARIO CRUD PARA MISIONES -->\r\n    <v-card outlined class=\"mb-5\">\r\n      <v-card-title>{{ editMisionMode ? 'Editar Misión' : 'Crear Misión' }}</v-card-title>\r\n      <v-card-text>\r\n        <v-form ref=\"misionForm\" v-model=\"misionValid\">\r\n          <v-text-field v-model=\"mision.descripcion\" label=\"Descripción\" required></v-text-field>\r\n          <v-text-field v-model=\"mision.estado\" label=\"Estado\" required></v-text-field>\r\n\r\n          <v-btn color=\"primary\" @click=\"saveMision\">\r\n            {{ editMisionMode ? 'Actualizar Misión' : 'Crear Misión' }}\r\n          </v-btn>\r\n          <v-btn text @click=\"clearMisionForm\">Cancelar</v-btn>\r\n        </v-form>\r\n      </v-card-text>\r\n    </v-card>\r\n\r\n    <!-- Tabla de Misiones -->\r\n    <h2>Lista de Misiones</h2>\r\n    <v-row dense>\r\n      <v-col v-for=\"mision in mis