>\r\n          <span class=\"headline\">Edit Equipo</span>\r\n        </v-card-title>\r\n        <v-card-text>\r\n          <v-form ref=\"editForm\">\r\n            <v-text-field\r\n              v-model=\"editedEquipo.tipo\"\r\n              label=\"Tipo\"\r\n              required\r\n            ></v-text-field>\r\n            <v-text-field\r\n              v-model=\"editedEquipo.descripcion\"\r\n              label=\"DescripciÃ³n\"\r\n              required\r\n            ></v-text-field>\r\n          </v-form>\r\n        </v-card-text>\r\n        <v-card-actions>\r\n          <v-spacer></v-spacer>\r\n          <v-btn color=\"blue darken-1\" text @click=\"closeEditDialog\">Cancel</v-btn>\r\n          <v-btn color=\"blue darken-1\" text @click=\"saveEdit\">Save</v-btn>\r\n        </v-card-actions>\r\n      </v-card>\r\n    </v-dialog>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nimport { ref } from 'vue';\r\nimport { useEquiposStore } from '../store/equipos';\r\n\r\nexport default {\r\n  setup() {\r\n    const equiposStore = useEquiposStore();\r\n    equiposStore.fetchEquipos();\r\n\r\n    const editDialog = ref(false);\r\n    const editedEquipo = ref(null);\r\n\r\n    const openEditDialog = (equipo) => {\r\n      editedEquipo.value = { ...equipo };\r\n      editDialog.value = true;\r\n    };\r\n\r\n    const closeEditDialog = () => {\r\n      editDialog.value = false;\r\n      editedEquipo.value = null;\r\n    };\r\n\r\n    const saveEdit = async () => {\r\n      if (editedEquipo.value) {\r\n        await equiposStore.updateEquipo(editedEquipo.value.id, editedEquipo.value);\r\n        closeEditDialog();\r\n      }\r\n    };\r\n\r\n    return {\r\n      equipos: equiposStore.equipos,\r\n      deleteEquipo: equiposStore.deleteEquipo,\r\n      editDialog,\r\n      editedEquipo,\r\n      openEditDialog,\r\n      closeEditDialog,\r\n      saveEdit\r\n    };\r\n  }\r\n};\r\n</script>"],"mappings":"AA6CA,SAASA,GAAE,QAAS,KAAK;AACzB,SAASC,eAAc,QAAS,kBAAkB;AAElD,e