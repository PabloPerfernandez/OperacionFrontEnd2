{"ast":null,"code":"import CrudForm from '@/components/CrudForm.vue';\nexport default {\n  components: {\n    CrudForm\n  },\n  data() {\n    return {\n      equipos: [],\n      selectedEquipo: {},\n      isEditing: false,\n      equipoFields: [{\n        name: 'equipoCodigo',\n        label: 'Código del Equipo',\n        rules: [v => !!v || 'El código es requerido']\n      }, {\n        name: 'tipo',\n        label: 'Tipo',\n        rules: [v => !!v || 'El tipo es requerido']\n      }]\n    };\n  },\n  mounted() {\n    this.fetchEquipos();\n  },\n  methods: {\n    async fetchEquipos() {\n      try {\n        const response = await fetch(\"http://localhost:5056/api/equipo\");\n        if (!response.ok) throw new Error(\"Error al obtener los equipos\");\n        this.equipos = await response.json();\n      } catch (error) {\n        console.error(\"Error:\", error);\n      }\n    },\n    handleSubmit(data) {\n      if (this.isEditing) {\n        this.updateEquipo(data);\n      } else {\n        this.createEquipo(data);\n      }\n    },\n    handleCancel() {\n      this.selectedEquipo = {};\n      this.isEditing = false;\n    },\n    editEquipo(equipo) {\n      this.selectedEquipo = {\n        ...equipo\n      };\n      this.isEditing = true;\n    },\n    async createEquipo(data) {\n      try {\n        const response = await fetch(\"http://localhost:5056/api/equipo\", {\n          method: 'POST',\n          headers: {\n            'Content-Type': 'application/json'\n          },\n          body: JSON.stringify(data)\n        });\n        if (response.ok) {\n          this.fetchEquipos();\n        }\n      } catch (error) {\n        console.error(\"Error:\", error);\n      }\n    },\n    async updateEquipo(data) {\n      try {\n        const response = await fetch(`http://localhost:5056/api/equipo/${data.equipoCodigo}`, {\n          method: 'PUT',\n          headers: {\n            'Content-Type': 'application/json'\n          },\n          body: JSON.stringify(data)\n        });\n        if (response.ok) {\n          this.fetchEquipos();\n        }\n      } catch (error) {\n        console.error(\"Error:\", error);\n      }\n    },\n    async deleteEquipo(codigo) {\n      try {\n        const response = await fetch(`http://localhost:5056/api/equipo/${codigo}`, {\n          method: 'DELETE'\n        });\n        if (response.ok) {\n          this.fetchEquipos();\n        }\n      } catch (error) {\n        console.error(\"Error:\", error);\n      }\n    }\n  }\n};","map":{"version":3,"names":["CrudForm","components","data","equipos","selectedEquipo","isEditing","equipoFields","name","label","rules","v","mounted","fetchEquipos","methods","response","fetch","ok","Error","json","error","console","handleSubmit","updateEquipo","createEquipo","handleCancel","editEquipo","equipo","method","headers","body","JSON","stringify","equipoCodigo","deleteEquipo","codigo"],"sources":["D:\\Smartway\\ByteStorm_Operacion\\bytestormoperacion\\bytestormfrontend\\src\\components\\Equipos.vue"],"sourcesContent":["<template>\r\n  <v-container>\r\n    <h1>Lista de Equipos</h1>\r\n\r\n    <!-- CRUD para Equipos -->\r\n    <crud-form\r\n      :entityName=\"'Equipo'\"\r\n      :formFields=\"equipoFields\"\r\n      :initialData=\"selectedEquipo\"\r\n      :isEditing=\"isEditing\"\r\n      @submit=\"handleSubmit\"\r\n      @cancel=\"handleCancel\"\r\n    />\r\n\r\n    <!-- Tarjetas Equipos -->\r\n    <v-row v-if=\"equipos.length > 0\" dense>\r\n      <v-col\r\n        v-for=\"equipo in equipos\"\r\n        :key=\"equipo.equipoCodigo\"\r\n        cols=\"12\"\r\n        sm=\"6\"\r\n        md=\"4\"\r\n      >\r\n        <v-card outlined>\r\n          <v-card-title>\r\n            Equipo Código: {{ equipo.equipoCodigo }}\r\n          </v-card-title>\r\n          <v-card-text>\r\n            Tipo: {{ equipo.tipo }}\r\n          </v-card-text>\r\n          <v-card-actions>\r\n            <v-btn color=\"primary\" @click=\"editEquipo(equipo)\">Editar</v-btn>\r\n            <v-btn color=\"error\" @click=\"deleteEquipo(equipo.equipoCodigo)\">Eliminar</v-btn>\r\n          </v-card-actions>\r\n        </v-card>\r\n      </v-col>\r\n    </v-row>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nimport CrudForm from '@/components/CrudForm.vue';\r\n\r\nexport default {\r\n  components: {\r\n    CrudForm,\r\n  },\r\n  data() {\r\n    return {\r\n      equipos: [],\r\n      selectedEquipo: {},\r\n      isEditing: false,\r\n      equipoFields: [\r\n        { name: 'equipoCodigo', label: 'Código del Equipo', rules: [v => !!v || 'El código es requerido'] },\r\n        { name: 'tipo', label: 'Tipo', rules: [v => !!v || 'El tipo es requerido'] },\r\n      ],\r\n    };\r\n  },\r\n  mounted() {\r\n    this.fetchEquipos();\r\n  },\r\n  methods: {\r\n    async fetchEquipos() {\r\n      try {\r\n        const response = await fetch(\"http://localhost:5056/api/equipo\");\r\n        if (!response.ok) throw new Error(\"Error al obtener los equipos\");\r\n        this.equipos = await response.json();\r\n      } catch (error) {\r\n        console.error(\"Error:\", error);\r\n      }\r\n    },\r\n    handleSubmit(data) {\r\n      if (this.isEditing) {\r\n        this.updateEquipo(data);\r\n      } else {\r\n        this.createEquipo(data);\r\n      }\r\n    },\r\n    handleCancel() {\r\n      this.selectedEquipo = {};\r\n      this.isEditing = false;\r\n    },\r\n    editEquipo(equipo) {\r\n      this.selectedEquipo = { ...equipo };\r\n      this.isEditing = true;\r\n    },\r\n    async createEquipo(data) {\r\n      try {\r\n        const response = await fetch(\"http://localhost:5056/api/equipo\", {\r\n          method: 'POST',\r\n          headers: { 'Content-Type': 'application/json' },\r\n          body: JSON.stringify(data),\r\n        });\r\n        if (response.ok) {\r\n          this.fetchEquipos();\r\n        }\r\n      } catch (error) {\r\n        console.error(\"Error:\", error);\r\n      }\r\n    },\r\n    async updateEquipo(data) {\r\n      try {\r\n        const response = await fetch(`http://localhost:5056/api/equipo/${data.equipoCodigo}`, {\r\n          method: 'PUT',\r\n          headers: { 'Content-Type': 'application/json' },\r\n          body: JSON.stringify(data),\r\n        });\r\n        if (response.ok) {\r\n          this.fetchEquipos();\r\n        }\r\n      } catch (error) {\r\n        console.error(\"Error:\", error);\r\n      }\r\n    },\r\n    async deleteEquipo(codigo) {\r\n      try {\r\n        const response = await fetch(`http://localhost:5056/api/equipo/${codigo}`, {\r\n          method: 'DELETE',\r\n        });\r\n        if (response.ok) {\r\n          this.fetchEquipos();\r\n        }\r\n      } catch (error) {\r\n        console.error(\"Error:\", error);\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n</style>"],"mappings":"AAyCA,OAAOA,QAAO,MAAO,2BAA2B;AAEhD,eAAe;EACbC,UAAU,EAAE;IACVD;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,EAAE;MACXC,cAAc,EAAE,CAAC,CAAC;MAClBC,SAAS,EAAE,KAAK;MAChBC,YAAY,EAAE,CACZ;QAAEC,IAAI,EAAE,cAAc;QAAEC,KAAK,EAAE,mBAAmB;QAAEC,KAAK,EAAE,CAACC,CAAA,IAAK,CAAC,CAACA,CAAA,IAAK,wBAAwB;MAAE,CAAC,EACnG;QAAEH,IAAI,EAAE,MAAM;QAAEC,KAAK,EAAE,MAAM;QAAEC,KAAK,EAAE,CAACC,CAAA,IAAK,CAAC,CAACA,CAAA,IAAK,sBAAsB;MAAE,CAAC;IAEhF,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,YAAY,CAAC,CAAC;EACrB,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,YAAYA,CAAA,EAAG;MACnB,IAAI;QACF,MAAME,QAAO,GAAI,MAAMC,KAAK,CAAC,kCAAkC,CAAC;QAChE,IAAI,CAACD,QAAQ,CAACE,EAAE,EAAE,MAAM,IAAIC,KAAK,CAAC,8BAA8B,CAAC;QACjE,IAAI,CAACd,OAAM,GAAI,MAAMW,QAAQ,CAACI,IAAI,CAAC,CAAC;MACtC,EAAE,OAAOC,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,QAAQ,EAAEA,KAAK,CAAC;MAChC;IACF,CAAC;IACDE,YAAYA,CAACnB,IAAI,EAAE;MACjB,IAAI,IAAI,CAACG,SAAS,EAAE;QAClB,IAAI,CAACiB,YAAY,CAACpB,IAAI,CAAC;MACzB,OAAO;QACL,IAAI,CAACqB,YAAY,CAACrB,IAAI,CAAC;MACzB;IACF,CAAC;IACDsB,YAAYA,CAAA,EAAG;MACb,IAAI,CAACpB,cAAa,GAAI,CAAC,CAAC;MACxB,IAAI,CAACC,SAAQ,GAAI,KAAK;IACxB,CAAC;IACDoB,UAAUA,CAACC,MAAM,EAAE;MACjB,I